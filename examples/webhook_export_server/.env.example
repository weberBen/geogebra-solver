# GeoGebra Export Server Configuration

# Server settings
HOST=0.0.0.0
PORT=8000
RELOAD=true

# CORS settings (comma-separated list of allowed origins)
# Use "*" for development, specify exact origins for production
CORS_ORIGINS=*

# API Key (optional, for authentication)
# Uncomment and set a strong secret key for production
# API_KEY=your-secret-key-here

# Processing limits
MAX_FILE_SIZE_MB=10
PROCESSING_TIMEOUT_SECONDS=30

# vpype default settings
DEFAULT_TOLERANCE=0.01mm
DEFAULT_UNITS=mm
DEFAULT_OPTIMIZE=true
